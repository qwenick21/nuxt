{
  "name": "nuxttest",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "rules": {
      "no-console": "off"
    }
  },
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
